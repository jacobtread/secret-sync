{
    "backend": {
        "provider": "aws"
    },
    "aws": {
        "profile": "example",
        "region": "ap-southeast-2",
        "endpoint": "https://secrets.example.com",
        "credentials": {
            "access_key_id": "test",
            "access_key_secret": "secret"
        }
    },
    "files": {
        "example": {
            "path": ".env",
            "secret": "example"
        },
        "example-2": {
            "path": ".env.secondary",
            "secret": "example-secondary"
        },
        "example-3": {
            "path": ".env.third",
            "secret": "example-third",
            "metadata": {
                "description": "Example Description"
            }
        },
        "example-4": {
            "path": ".env.forth",
            "secret": "example-forth",
            "metadata": {
                "description": "Example Description",
                "tags": {
                    "environment": "Production"
                }
            }
        },
        "example-5": {
            "path": ".env.fifth",
            "secret": "example-fifth",
            "metadata": {
                "description": "Example Description",
                "tags": {
                    "environment": "Production",
                    "test-key": "test-value"
                }
            }
        }
    }
}
